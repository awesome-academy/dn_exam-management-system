<div class="row function-name">
  <div class="col-md-10 offset-md-1 col-12 pt-3 pb-3">
    <p class="function-text"><%= t ".title" %></p>
  </div>
</div>
<div class="row mt-2">
  <div class="col-md-10 offset-md-1">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render "devise/shared/error_messages", resource: resource %>
      <%= f.label :email, t(".email") %>
      <%= f.email_field :email, readonly: true, class: "form-control" %>
      <%= f.label :first_name, t(".first_name") %>
      <%= f.text_field :first_name, autofocus: true, class: "form-control" %>
      <%= f.label :last_name, t(".last_name") %>
      <%= f.text_field :last_name, autofocus: true, class: "form-control" %>
      <%= f.label :password, t(".password") %>
      <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> <%= t "devise.registrations.new.pwd_min_length" %> )</em>
      <% end %><br />
      <%= f.password_field :password, class: "form-control" %>
      <%= f.label :password_confirmation, t(".password_confirmation") %><br />
      <%= f.password_field :password_confirmation, class: "form-control" %>
      <%= f.label :current_password, t(".password_current") %><br />
      <%= f.password_field :current_password, class: "form-control" %>
      <div class="row mt-2">
        <div class="col text-center">
          <%= f.submit t(".btn_edit"), class: "btn btn-primary " %>
          <%= link_to t(".cancel_update_profile"), :back, class: "btn btn-warning" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
